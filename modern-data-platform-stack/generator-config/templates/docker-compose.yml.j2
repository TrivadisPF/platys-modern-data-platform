{% if ATLAS_enable  | default(false) %}
  #  ================================== Apache Atlas ========================================== #
  atlas:
    image: trivadis/apache-atlas:{{__ATLAS_version}}
    container_name: atlas
    hostname: atlas
    labels:
      com.platys.name: "atlas"
